<?xml version="1.0" encoding="UTF-8"?>
<c:config xmlns:c="http://www.sourceid.org/2004/05/config">

    <!-- The key length in bits of the AES encryption keys used -->
    <c:item name="key-length">128</c:item>

    <!-- How many keys to keep around -->
    <c:item name="max-number-of-keys">3</c:item>

    <!-- How often to issue a new key in seconds -->
    <c:item name="key-rolling-period">28800</c:item>
</c:config>