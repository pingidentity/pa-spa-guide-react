<?xml version="1.0" encoding="UTF-8"?>
<con:config xmlns:con="http://www.sourceid.org/2004/05/config">
    <con:map name="signature-algorithms">
        <con:item name="DSA_SHA1">http://www.w3.org/2000/09/xmldsig#dsa-sha1</con:item>
        <con:item name="RSA_SHA1">http://www.w3.org/2000/09/xmldsig#rsa-sha1</con:item>
        <con:item name="RSA_SHA256">http://www.w3.org/2001/04/xmldsig-more#rsa-sha256</con:item>
        <con:item name="RSA_SHA384">http://www.w3.org/2001/04/xmldsig-more#rsa-sha384</con:item>
        <con:item name="RSA_SHA512">http://www.w3.org/2001/04/xmldsig-more#rsa-sha512</con:item>
        <con:item name="ECDSA_SHA256">http://www.w3.org/2001/04/xmldsig-more#ecdsa-sha256</con:item>
        <con:item name="ECDSA_SHA384">http://www.w3.org/2001/04/xmldsig-more#ecdsa-sha384</con:item>
        <con:item name="ECDSA_SHA512">http://www.w3.org/2001/04/xmldsig-more#ecdsa-sha512</con:item>
    </con:map>
    
    <con:map name="signature-algorithms-runtime">
        <con:item name="SHA1withDSA">http://www.w3.org/2000/09/xmldsig#dsa-sha1</con:item>
        <con:item name="SHA1withRSA">http://www.w3.org/2000/09/xmldsig#rsa-sha1</con:item>
        <con:item name="SHA256withRSA">http://www.w3.org/2001/04/xmldsig-more#rsa-sha256</con:item>
        <con:item name="SHA384withRSA">http://www.w3.org/2001/04/xmldsig-more#rsa-sha384</con:item>
        <con:item name="SHA512withRSA">http://www.w3.org/2001/04/xmldsig-more#rsa-sha512</con:item>
        <con:item name="SHA256withECDSA">http://www.w3.org/2001/04/xmldsig-more#ecdsa-sha256</con:item>
        <con:item name="SHA384withECDSA">http://www.w3.org/2001/04/xmldsig-more#ecdsa-sha384</con:item>
        <con:item name="SHA512withECDSA">http://www.w3.org/2001/04/xmldsig-more#ecdsa-sha512</con:item>
    </con:map>
    
    <con:map name="digest-algorithms">
        <con:item name="DSA_SHA1">http://www.w3.org/2000/09/xmldsig#sha1</con:item>
        <con:item name="RSA_SHA1">http://www.w3.org/2000/09/xmldsig#sha1</con:item>
        <con:item name="RSA_SHA256">http://www.w3.org/2001/04/xmlenc#sha256</con:item>
        <con:item name="RSA_SHA384">http://www.w3.org/2001/04/xmldsig-more#sha384</con:item>
        <con:item name="RSA_SHA512">http://www.w3.org/2001/04/xmlenc#sha512</con:item>
        <con:item name="ECDSA_SHA256">http://www.w3.org/2001/04/xmlenc#sha256</con:item>
        <con:item name="ECDSA_SHA384">http://www.w3.org/2001/04/xmldsig-more#sha384</con:item>
        <con:item name="ECDSA_SHA512">http://www.w3.org/2001/04/xmlenc#sha512</con:item>
    </con:map>
</con:config>
